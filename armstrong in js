<!DOCTYPE html>
<html lang="en">
<head>
    
</head>
<body>
    <script>
        var n,c,arm=0,r;
        var n=prompt("Enter Number");
        c=n;
        while(n>0)
        {
            r=n%10;
            arm=(r*r*r)+arm;
            n=parseInt(n/10);
        }
        if(c==arm)
        {
            document.write("armstrong");
        }
        else{
            document.write("not");
        }
    </script>
</body>
</html>
