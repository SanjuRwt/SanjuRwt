<html>
    <head>
    <script>
        function add(){
            var num1=document.getElementById("num1").value;
            var num2=document.getElementById("num2").value;

            var tot=parseInt(num1) + parseInt(num2);
            document.getElementById("result").innerHTML=tot;
        }
    </script>
    </head>
    <body>
        <h1>Add two numbers</h1><br><br>
        num1<input id="num1"><br><br>
        num2<input id="num2"><br><br>
        Result:<h2 id="result"></h2><br><br>
        <button onclick="add()">Add</button>
    </body>
</html>
