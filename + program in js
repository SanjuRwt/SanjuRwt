<html>
    <script>
        function ppp(){
            var num1=document.getElementById("num1").value;
            var num2=document.getElementById("num2").value;

            var t=parseInt(num1)+parseInt(num2);
            document.getElementById("res").innerHTML=t;
        }
    </script>
<body>
   num1<input id="num1"><br>
   num2<input id="num2"><br>
   <h1>Result</h1>
   <h2 id="res"></h2>
   <button onclick="ppp()">+</button>
</body>
</html>
